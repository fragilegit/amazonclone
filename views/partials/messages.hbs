{{!-- {{#if typeof errors != 'undefined' }} --}}
{{#if messages }}
    {{#each messages}}
    <div class="alert alert-{{this.status}} alert-dismissible fade show" role="alert">
        {{this.msg}}
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
{{/each}}
{{/if}}